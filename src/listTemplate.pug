//- template.pug

doctype html
html
  head
    link(href='/static/style.css' type='text/css' rel='stylesheet')
  body
    h1 #{title}
    a(class='reportLink' href='/pipelines') Pipelines
    ul.linkWrapper
      each val in nameList
        li(class='linkRow')
          a(class='reportLink' href='/'+val) #{val}
          a(class='imgLink' href='/images/'+val) images
          a(class='imgLink' href='/screenshoots/'+val) screenshoots