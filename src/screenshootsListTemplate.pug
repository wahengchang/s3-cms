//- template.pug

doctype html
html
  head
    link(href='/static/style.css' type='text/css' rel='stylesheet')
  body
    h1 #{title}
    div.screenshootWrapper
      each val in pngFileList
        div(class='screenshootRow')
          p #{val.title}
          img(class='screenshootBlock' src=val.url)
          